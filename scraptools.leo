<t tx="zoomq.20140226204618.4739">def _uli_all_item(drdf, seq):
    if types.ListType is type(seq):
        for seqid in seq:
            if seqid in drdf['SEQ']:
                yield '''&lt;LI&gt;&lt;a class="folder"&gt;
                &lt;img src="./folder.png" width="16" height="16" alt=""&gt;
