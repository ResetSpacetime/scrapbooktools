                for ul in _uli_all_item(drdf, drdf['SEQ'][seqid]):
                    yield ul
                yield "&lt;/UL&gt;&lt;/LI&gt;"
            else:
                for li in _uli_all_item(drdf, seqid):
                    yield li
    else:
        if seq in drdf['SEQ']:
            yield "&lt;LI&gt;%s&lt;UL&gt;" % drdf['DESC'][seq]['title']
            for ul in _uli_all_item(drdf, drdf['SEQ'][seq]):
